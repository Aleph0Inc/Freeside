%# $Header: /home/cvs/cvsroot/freeside/rt/webrt/Elements/Attic/Checkbox,v 1.1 2002-08-12 06:17:08 ivan Exp $
<INPUT TYPE="Checkbox" NAME ="<%$Name%>" <%$IsChecked%>>

<%ARGS>
$Name => undef
$Default => undef
$True => undef
$False => undef
$IsChecked => undef
</%ARGS>

<%INIT>
$IsChecked = 
  ($Default && $Default =~ /checked/i)
    ? " CHECKED " : "";
1;
</%INIT>
