      <param name="vertical-label"  value="pps"/>
      <param name="rrd-ds"          value="PacketsWred_%sce-tp-index%"/>
      <param name="snmp-object"
            value="$tpTotalNumWredDiscardedPackets.%sce-tp-index%"/>
