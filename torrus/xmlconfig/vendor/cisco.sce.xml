        <param name="graph-legend"    value="subs"/>
        <param name="rrd-ds"          value="NumWithSessions"/>
        <param name="snmp-object"     value="$subNumWithSessions"/>
      </leaf>
      
    </subtree>
  </template>

  <template name="cisco-sce-tp-subtree">
    <param name="has-overview-shortcuts" value="yes"/>
